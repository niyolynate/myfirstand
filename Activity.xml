<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">

    <EditText
        android:id="@+id/editTextTextPersonName4"
        android:layout_width="231dp"
        android:layout_height="65dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Name"
        tools:layout_editor_absoluteX="214dp"
        tools:layout_editor_absoluteY="269dp" />

    <Button
        android:id="@+id/button5"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:text="First Name" />

    <EditText
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="238dp"
        android:layout_height="51dp"
        android:ems="10"
        android:inputType="textPersonName"
        android:text="Last Name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        tools:layout_editor_absoluteX="217dp" />

    <Button
        android:id="@+id/button3"
        android:layout_width="198dp"
        android:layout_height="61dp"
        android:layout_marginStart="4dp"
        android:text="First Name"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="273dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Login Form"
        android:textSize="34sp"
        android:textStyle="@string/app_name"
        tools:layout_editor_absoluteX="108dp"
        tools:layout_editor_absoluteY="13dp" />

    <EditText
        android:id="@+id/editPassword"
        android:layout_width="234dp"
        android:layout_height="47dp"
        android:ems="10"
        android:inputType="numberPassword"
        tools:layout_editor_absoluteX="206dp"
        tools:layout_editor_absoluteY="475dp" />

    <Button
        android:id="@+id/button"
        android:layout_width="176dp"
        android:layout_height="50dp"
        android:layout_marginStart="16dp"
        android:text="Password"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="475dp" />

    <Button
        android:id="@+id/button2"
        android:layout_width="176dp"
        android:layout_height="61dp"
        android:layout_marginStart="16dp"
        android:text="Email"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.599" />

    <EditText
        android:id="@+id/editTextTextEmailAddress"
        android:layout_width="222dp"
        android:layout_height="46dp"
        android:ems="10"
        android:inputType="textEmailAddress"
        tools:layout_editor_absoluteX="202dp"
        tools:layout_editor_absoluteY="416dp" />

    <RadioGroup
        android:id="@+id/radioGroup"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        tools:layout_editor_absoluteX="45dp"
        tools:layout_editor_absoluteY="601dp" />

    <SearchView
        android:id="@+id/searchView"
        android:layout_width="167dp"
        android:layout_height="48dp"
        tools:layout_editor_absoluteX="43dp"
        tools:layout_editor_absoluteY="580dp" />

    <EditText
        android:id="@+id/Website"
        android:layout_width="245dp"
        android:layout_height="58dp"
        android:ems="10"
        android:inputType="textPostalAddress"
        tools:layout_editor_absoluteX="248dp"
        tools:layout_editor_absoluteY="571dp" />

    <RadioButton
        android:id="@+id/radioButton"
        android:layout_width="77dp"
        android:layout_height="48dp"
        android:text="Adult"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="663dp"
        tools:shadowColor="@color/purple_200" />

    <RadioButton
        android:id="@+id/radioButton2"
        android:layout_width="77dp"
        android:layout_height="48dp"
        android:text="18 and Bellow"
        tools:layout_editor_absoluteX="60dp"
        tools:layout_editor_absoluteY="742dp" />

    <Button
        android:id="@+id/button6"
        android:layout_width="117dp"
        android:layout_height="48dp"
        android:text="Register"
        tools:layout_editor_absoluteX="358dp"
        tools:layout_editor_absoluteY="742dp" />

    <Button
        android:id="@+id/button7"
        android:layout_width="202dp"
        android:layout_height="53dp"
        android:layout_marginStart="4dp"
        android:text="Last Name"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="134dp"
        android:layout_height="117dp"
        tools:layout_editor_absoluteX="174dp"
        tools:layout_editor_absoluteY="85dp"
        tools:src="@tools:sample/avatars" />

    <Button
        android:id="@+id/button8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Add Image"
        tools:layout_editor_absoluteX="182dp"
        tools:layout_editor_absoluteY="170dp" />

</androidx.constraintlayout.widget.ConstraintLayout>
